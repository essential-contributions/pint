// Generate a random 18-hole golf course.

let h: int[18];

constraint forall i in 0..17 {
    h[i] >= 3 && h[i] <= 5
};

let sum: int = h[0] + h[1] + h[2] + h[3] + h[4] + h[5] + h[6]
             + h[7] + h[8] + h[9] + h[10] + h[11] + h[12] + h[13]
             + h[14] + h[15] + h[16] + h[17];

constraint sum == 72;

solve satisfy;
