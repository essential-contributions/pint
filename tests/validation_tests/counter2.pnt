// db <<<
// 14 14 14 14, 42
// >>>

intent Initialize {
    let value: int;
    state counter: int = __storage_get([14, 14, 14, 14]);
    constraint counter' == value;
}

intent Increment {
    let amount: int;
    state counter: int = __storage_get([14, 14, 14, 14]);
    constraint counter' == counter + amount;
}
