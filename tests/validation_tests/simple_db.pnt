    constraint x' - x == 1; // x' == 43
    constraint y' == y + 4; // y' == 47
    constraint x' + y' > 89 && x' * y' > 1932;
    constraint x < y;
    constraint w + u + v == 2206;
    constraint external_1' - external_1 == 1;
    constraint external_2' - external_2 == 1;
