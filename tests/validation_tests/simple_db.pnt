predicate Simple {
    state x: int = storage::x;
    state y: int = storage::y;
    state z: int = storage::z;

    interface FooInstance = Foo(0xEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEE);
    state external_1 = FooInstance::storage::y; // 69

    state external_2 = __storage_get_extern(
        0xEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEE,
        [1],
    ); // 69

    var w: int = x + 99; // 141
    var u: int = y * 3;  // 129
    var v: int = z * z;  // 1936
    var e_old = external_1;
    var e_new = external_2';

    var a: int[5];

    constraint forall i in 0..4 {
        a[i] == i*i
