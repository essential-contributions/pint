intent Simple {
    pub var x: int;
    pub var y: int;
    pub var w: b256;
    pub var b: bool;

    var z: int;
    var q: int;

    constraint x + z == y + q;
    constraint w == 0x0000000000000009000000000000000900000000000000090000000000000009;
    constraint b == true;
    constraint !b == false;
}
