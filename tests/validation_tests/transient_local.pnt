    constraint c.0 == 42;
    constraint c.1 == { 43, 0x0000000000000008000000000000000800000000000000080000000000000008};
    constraint c.1.0 == 43;
    constraint c.1.1 == 0x0000000000000008000000000000000800000000000000080000000000000008;
