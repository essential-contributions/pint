// 0xEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEE, 0, 42
// 0xEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEE, 1, 69
// 0xDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDD, 0, 99
// 0xDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDD, 1, 166
// >>>

// from external1.pnt
extern Extern1(0xEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEE) {
    storage {
        x: int,
        y: int
    }
}

// from external0.pnt
extern Extern2(0xDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDD) {
    storage {
        w: int,
        z: int
    }
}

intent Foo {
    state x = Extern1::storage::x;
    state y = Extern1::storage::y;
    state w = Extern2::storage::w;
    state z = Extern2::storage::z;

    constraint x' - x == 1;
    constraint y' - y == 2;
    constraint w' - w == 3;
    constraint z' - z == 4;
}
