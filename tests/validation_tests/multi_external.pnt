// db <<<
// 0xEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEE, 0, 42
// 0xEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEE, 1, 69
// 0xDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDD, 0, 99
// 0xDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDD, 1, 166
// >>>

// from external1.pnt
interface Bar {
    storage {
        x: int,
        y: int
    }
}

// from external0.pnt
interface Baz {
    storage {
        w: int,
        z: int
    }
}

predicate Foo() {
    state bar_addr = 0xEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEE;
    state baz_addr = 0xDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDD;

    state x = Bar[[bar_addr]]::storage::x;
    state y = Bar[[bar_addr]]::storage::y;
    state w = Baz[[baz_addr]]::storage::w;
    state z = Baz[[baz_addr]]::storage::z;

    constraint x' - x == 1;
    constraint y' - y == 2;
    constraint w' - w == 3;
    constraint z' - z == 4;
}
