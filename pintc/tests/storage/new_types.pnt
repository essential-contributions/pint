type OneOrTheOther = bool;
type Count = int;
type Haaash = b256;

storage {
    m: ( OneOrTheOther => int ),
    n: ( bool => Count ),
    o: ( OneOrTheOther => Haaash ),
}

predicate test() {
    let x = storage::m[true];
    let y = storage::n[false];
    let z = storage::o[false];

    constraint x == 11;
    constraint y' < 22;
    constraint z != 0x3333333333333333333333333333333333333333333333333333333333333333;
}

