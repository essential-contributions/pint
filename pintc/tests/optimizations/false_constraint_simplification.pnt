predicate Test {
    var x = 1;
    var y: int;

    // non-trivial that should be simplified when a constraint resolves to false
    constraint x < y;
    constraint x == x;

    // resolves to false
    constraint 1 + 2 < 3;
    constraint (false && (::x == ::y));
    constraint ((::x == ::y) && false);

    if true {
        // non-trivial
        constraint y == 3;

        // resolves to false
        constraint 3 < 2;
    }
}

