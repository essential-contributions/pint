const test: int = 20;
const test_array: int[2] = [1, 2];
const test_tuple: { bool, int } = { false, 5 };

predicate Test {
    var a = 1 + 2;
    var b = 1 - test_array[0];
    var c = (1 + 2) - 3 + 4 - (2 - test_tuple.1);
    var d = !(1 > 2);
    var e = -(test + 3);
    var f = d ? false : !(!true);
    
    var y: int;
    if (1 + 2) - 3 + 4 - (2 - 5) < 5 {
        constraint 3 - 4 + 1 == y;
    } else {
        constraint y in [1, 4, 7];
    }

    var x: int;
    if true && y == 0 {
        constraint x == 1;
    } else {
        constraint x == 0;
    }

    if false && y == 0 {
        constraint x == 1;
    } else {
        constraint x == 0;
    }

    if true || y == 0 {
        constraint x == 1;
    } else {
        constraint x == 0;
    }

    if false || y == 0 {
        constraint x == 1;
    } else {
        constraint x == 0;
    }
}

