enum MyEnum = A | B;

storage {
    a: int,
    b: bool,
    c: b256,
    d: { int, bool },
}

predicate test {
    state a = storage::a;
    state b = storage::b;
    state c = storage::c;
    state d = storage::d;

    var a_len = __state_len(a);
    var b_len = __state_len(b);
    var c_len = __state_len(c);
    var d_len = __state_len(d);
}

