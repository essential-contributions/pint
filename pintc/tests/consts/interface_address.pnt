interface Counter {
    storage {
        counter: int,
    }
}

const ContractID: b256 = 0x0003000300030003000300030003000300030003000300030003000300030003;

predicate Foo() {
    interface CounterInstance = Counter(ContractID);
    state counter = CounterInstance::storage::counter;
}

