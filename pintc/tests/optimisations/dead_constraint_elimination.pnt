const a = true;
const b = 3.14 < 4.23;
const c = { 1, 2 };
const d: int[5] = [0, 1, 2, 3, 4];

predicate Test {
    var x = 1;
    var y: int;

    // trivial and should be removed
    constraint true;
    constraint false;
    constraint 1 + 2 == 3;
    constraint a;
    constraint b;
    constraint c.0 > d[1];
    constraint d[0] < c.1;

    // non-trivial that should be kept for now
    constraint x < y;
    constraint x == x;
}

