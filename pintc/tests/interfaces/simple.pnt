interface Foo {
    storage {
        x: int,
        y: int,
    }

    intent I1 {
        pub var x1: int;
        pub var x2: b256;
        pub var x3: bool;
    }

    intent I2 {
        pub var x1: int;
        pub var x2: b256;
        pub var x3: bool;
    }

    intent I3;
}

interface Bar {
    storage {
        x: int,
        z: b256,
    }

    intent I1 {
        pub var x1: int;
        pub var x2: b256;
        pub var x3: bool;
    }

    intent I2 {
        pub var x1: int;
        pub var x2: b256;
        pub var x3: bool;
    }

    intent I3;
}

intent Simple {
    var addr1: b256;
    var addr2: b256;
    var addr3: b256;
    var addr4: b256;

    interface FooInstance = Foo(addr1);
    interface BarInstance = Bar(addr2);

    state x = FooInstance::storage::x;
    state y = FooInstance::storage::y;
    state z = BarInstance::storage::z;

    intent FooI1Instance1 = FooInstance::I1(addr3);
    intent FooI1Instance2 = FooInstance::I1(addr3);

    intent BarI2Instance1 = BarInstance::I2(addr4);
    intent BarI2Instance2 = BarInstance::I2(addr4);
    intent BarI2Instance3 = BarInstance::I2(addr4);

    var x1 = FooI1Instance1::x1;
    var x2 = BarI2Instance2::x2;
    var x3 = BarI2Instance3::x3;
}

