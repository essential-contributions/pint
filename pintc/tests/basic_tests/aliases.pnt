type MyInt = int;
type MyReal = real;
type MyBool = bool;
type MyTuple = { int, int };
type MyArray = bool[5];
type MyMap = (b256 => int);
type MyB256 = b256;
type MyNestedAlias = MyInt;
type MyAliasedMap = (MyB256 => MyTuple);
type MyComplexMap = (MyBool => MyMap);

storage {
    x: MyMap,
    y: MyAliasedMap,
    z: MyComplexMap,
}

predicate test {
   var m: MyInt;
   var n: MyReal;
   var o: MyBool;
   var p: MyTuple;
   var q: MyArray;
   var r: MyB256;
   var s: MyNestedAlias;
}

