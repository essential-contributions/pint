storage {
    x: int,
    z: {int, int, int},
}

predicate test {
    state x = storage::x;
    state z = storage::z;

    constraint nil == nil;
    constraint nil != nil;

    constraint x == nil;
    constraint nil == x;

    constraint z == nil;
    constraint nil == z;

    constraint x != nil;
    constraint nil != x;

    constraint z != nil;
    constraint nil != z;

    constraint (nil == z) && (nil != x);
    constraint (nil != z) && (nil == x);
}

