enum TheLetterA = T | U | V;
type Hooray = int[3];
type Point = { x: real, y: real };
enum Experience = Real | Unreal;
type A = TheLetterA;
type I = int;
type Real = real;

storage {
    x: int,
}

predicate test {
    var t: A = A::T;
    var u: TheLetterA = A::U;
    var v: A = TheLetterA::V;

    var i: I = 11;
    var j: {I, real} = {22, 33.3};

    var r: Real = 44.4;
    var e: Experience = Experience::Real;

    var h: Hooray;
    constraint h[1] == 55;

    var p: Point = { 66.6, 77.7 };
    constraint r < p.y;

    var q: real = i as Real;

    state x: I = storage::x;
}

