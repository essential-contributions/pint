union TheLetterA = T | U | V;
type Hooray = int[3];
type Point = { x: int, y: int };
union Experience = Int | Unint;
type A = TheLetterA;
type I = int;
type Int = int;

const cc: I = 4;

storage {
    x: int,
    c: I,
}

interface Boo {
    storage {
        c: I,
    }

    predicate Foo(x: A);
}

predicate test(h: Hooray, pt: A) {
    let t: A = A::T;
    constraint pt == A::T;

    let u: TheLetterA = A::U;
    let v: A = TheLetterA::V;

    let i: I = 11;
    let j: {I, int} = {22, 33};

    let r: Int = 44;
    let e: Experience = Experience::Int;

    constraint h[1] == 55;

    let p: Point = { 66, 77 };
    constraint r < p.y;

    let q: int = i as Int;

    let x: I = storage::x;
}

